{
  "name": "lightdetective",
  "version": "1.0.0",
  "description": "A client-side preset extractor for Adobe Lightroom adjustments",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/leifniem/LightDetective.git"
  },
  "scripts": {
    "dev": "parcel src/index.html src/results-testing.html --global Transfer",
    "build": "rm -rf dist; parcel build src/index.html --global Transfer"
  },
  "keywords": [
    "lightroom",
    "exif",
    "preset"
  ],
  "author": "Leif Niemczik",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.12.3"
  },
  "dependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "autoprefixer": "^9.6.1",
    "exifreader": "^2.8.2",
    "file-saver": "^2.0.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      "env"
    ]
  }
}
